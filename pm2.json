{
  "apps": {
    "name": "kkapi-open",
    "script": "./dist/main.js",
    "cwd": "./",
    "exec_mode": "cluster_mode",
    "instances": 0,
    "max_memory_restart": "512M",
    "error_file": "./logs/app-err.log",
    "out_file": "./logs/app-out.log",
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "min_uptime": "60s",
    "max_restarts": 30,
    "autorestart": true,
    "cron_restart": "",
    "restart_delay": "60s"
  }
}
